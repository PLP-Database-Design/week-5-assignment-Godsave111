<table>
    <thead>
        <tr>
            <th>patients_id</th>
            <th>first name</th>
            <th>last name</th>
            <th>Date of birth</th>
            <th>language</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row => { %>
            <tr>
                <td><%= row.patient_id %></td>
                <td><%= row.first_name %></td>
                <td><%= row.last_name %></td>
                 <!-- <td><%= row.date_of_birth %></td> -->
                  <td>
                    <%= new Date(row.date_of_birth).toLocaleDateString('en-CA') %>
                  </td>
                  <td><%= row.gender %></td>
                  <td><%= row.language %></td>
                
            </tr>
        <% }); %>
    </tbody>
  </table>
  